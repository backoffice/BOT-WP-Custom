{
    "name": "pantheon-upstreams/upstream-configuration",
    "type": "project",
    "version": "dev-main",
    "repositories": {
        "satispress": {
            "type": "composer",
            "url": "https://dev-bot-satispress.pantheonsite.io/satispress/",
            "options": {
                "http": {
                    "header": ["Deterrence-Bypass: 1"]
                }
            }
        },
        "wppackagist": {
            "type": "composer",
            "url": "https://wpackagist.org"
        }
    },
    "require": {
        "wpackagist-plugin/akismet": "^5.5.0",
        "wpackagist-plugin/autoptimize": "^3.1.13",
        "wpackagist-plugin/better-search-replace": "*",
        "wpackagist-plugin/content-views-query-and-display-post-page": "*",
        "wpackagist-plugin/duplicate-page": "^4.5.5",
        "wpackagist-plugin/duracelltomi-google-tag-manager": "^1.22.1",
        "wpackagist-plugin/easy-smooth-scroll-links": "^2.23.1",
        "wpackagist-plugin/enable-media-replace": "4.1.7",
        "wpackagist-plugin/filebird": "*",
        "wpackagist-plugin/font-awesome": "^4.5.0",
        "wpackagist-plugin/gdpr-cookie-compliance": "^4.16.1",
        "wpackagist-plugin/genesis-blocks": "*",
        "wpackagist-plugin/gutenslider": "^6.1.2",
        "wpackagist-plugin/jetpack-boost": "^3.13.1",
        "wpackagist-plugin/leadin": "^11.3.21",
        "wpackagist-plugin/lh-hsts": "^1.25",
        "wpackagist-plugin/modal-block": "^2.1",
        "wpackagist-plugin/pods": "^3.3.4",
        "wpackagist-plugin/popup-anything-on-click": "^2.8.9",
        "wpackagist-plugin/redirection": "5.5.2",
        "wpackagist-plugin/search-filter": "^1.2.18",
        "wpackagist-plugin/socialsnap": "^1.4",
        "wpackagist-plugin/svg-support": "^2.5.14",
        "wpackagist-plugin/ultimate-post": "^4.1.38",
        "wpackagist-plugin/wordpress-seo": ">=24.5",
        "wpackagist-plugin/wp-cfm": "^1.7.10",
        "wpackagist-plugin/wp-megamenu": "^1.4.2",
        "wpackagist-plugin/wp-smushit": "^3.21.1",
        "wpackagist-theme/twentynineteen": "^1.9",
        "backoffice/advanced-custom-fields-pro": "^6.4.3",
        "backoffice/pt-content-views-pro": "^7.1",
        "backoffice/search-filter-pro": "^2.5.16",
        "wpackagist-plugin/ninja-forms": "3.13.0",
        "wpackagist-plugin/ga-google-analytics": "20250326",
        "wpackagist-plugin/cleantalk-spam-protect": "6.66",
        "wpackagist-plugin/mailin": "3.2.8",
        "wpackagist-plugin/weglot": "5.2",
        "wpackagist-plugin/wordfence": "8.1.0",
        "wpackagist-plugin/google-site-kit": "1.164.0"
    },
    "config": {
        "allow-plugins": true,
        "prefer-stable": true
    }
}
